//这只是一个随手的foolish solution，yangwuqi
//20ms，faster than 100%

func romanToInt(s string) int {
    var sum int
    for i:=0;i<len(s);i++{
        var flag int
        if s[i]=='C'{
            for i<len(s)-1&&s[i]=='C'{
                i++
                flag++
            }
            if s[i]=='C'{
                sum+=(flag+1)*100
                break
            }
            if s[i]=='M'{
                sum+=1000-flag*100    
            }else if s[i]=='D'{
                sum+=500-flag*100
            }else{
                sum+=flag*100
                i--
            }
        }else if s[i]=='X'{
            for i<len(s)-1&&s[i]=='X'{
                i++
                flag++
            }
            if s[i]=='X'{
                sum+=(flag+1)*10
                break
            }
            if s[i]=='C'{
                sum+=100-flag*10    
            }else if s[i]=='L'{
                sum+=50-flag*10
            }else{
                sum+=flag*10
                i--
            }
        }else if s[i]=='I'{
            for i<len(s)-1&&s[i]=='I'{
                i++
                flag++
            }
            if s[i]=='I'{
                sum+=(flag+1)*1
                break
            }
            if s[i]=='X'{
                sum+=10-flag    
            }else if s[i]=='V'{
                sum+=5-flag
            }else{
                sum+=flag
                i--
            }
        }else if s[i]=='M'{
            sum+=1000
        }else if s[i]=='D'{
            sum+=500
        }else if s[i]=='L'{
            sum+=50
        }else if s[i]=='V'{
            sum+=5
        }
    }
    return sum
}
